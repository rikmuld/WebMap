#searchbar
    width calc(100vw - 58px)

#logincard
    padding-top 30px
    padding-bottom 30px
    border-radius: 4px
    @media only screen and (orientation: portrait)
        width 250px

#webmap
    font-family: 'Roboto', sans-serif;
    text-align center
    display block
    font-size 2rem
    font-weight 200 
    color white
    margin-bottom 100px
    margin-top 10px
    @media only screen and (max-height: 380px)
        margin-bottom 30px
    @media only screen and (max-height: 450px) and (min-height 380px)
        margin-bottom 55px
    @media only screen and (max-height: 520px) and (min-height 450px)
        margin-bottom 80px

#logo
    width 130px
    display block
    height auto
    margin-left auto
    margin-right auto
    border-radius 13%
    